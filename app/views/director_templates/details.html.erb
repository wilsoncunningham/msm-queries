<h1>Director #<%= @director_id %> details</h1>

<hr>

<dl>
  <dt>Name</dt>
    <dd><%= @director.name %></dd>
  <dt>Dob</dt>
    <dd><%= @director.dob %></dd>
  <dt>Bio</dt>
    <dd><%= @director.bio %></dd>
  <dt>Image</dt>
    <dd><img src="<%= @director.image %>"></dd>
  <dt>Created at</dt>
    <dd><%= @director.created_at %></dd>
  <dt>Updated at</dt>
    <dd><%= @director.updated_at %></dd>

</dl>
